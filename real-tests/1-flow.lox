print "--- IFs ---";

if (true) {
    print "OK";
} else {
    print "ERROR";
}

if (false) {
    print "ERROR";
} else {
    print "OK";
}

print "--- WHILEs ---";
var i = 0;
var match = 0;
while (i <= 3) {
    match = i;
    i = i + 1;
}

if (match == 3) {
    print "OK";
} else {
    print "ERROR";
}

print "--- FOR ---";
var match = 0;
for (var i = 0 ; i <= 5; i = i + 1) {
    match = i;
}

if (match == 5) {
    print "OK";
} else {
    print "ERROR";
}
